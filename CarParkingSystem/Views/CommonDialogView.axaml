<UserControl x:Class="CarParkingSystem.Views.CommonDialogView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:suki="https://github.com/kikipoulet/SukiUI"
			 xmlns:vm="using:CarParkingSystem.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:i18n="https://codewf.com"
			 xmlns:lang="clr-namespace:CarParkingSystem.I18n;assembly=CarParkingSystem"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:DataType="vm:CommonDialogViewModel"
             mc:Ignorable="d">
    <suki:GroupBox Header="{Binding Title}">
		<StackPanel Margin="20,50" HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBlock Text="{Binding Message}" VerticalAlignment="Center"/>
			<!--<TextBlock Text="" VerticalAlignment="Center"/>-->
		</StackPanel>
    </suki:GroupBox>
</UserControl>
